{
  "name": "esml",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "watch": "tsc --build --watch",
    "build": "tsc -b",
    "install": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "files": [
    "src/**/*",
    "tsconfig.json",
    "tsconfig.importmap.json"
